{
  "name": "passwordy",
  "version": "1.0.1",
  "description": "A simple secure password generator",
  "main": "bin/index.js",
  "preferGlobal": true,
  "bin": {
    "passwordy": "bin/index.js"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "tag": "latest"
  },
  "scripts": {
    "start": "node .",
    "commit": "cz",
    "release": "semantic-release"
  },
  "author": "F2BEAR",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/F2BEAR/passwordy"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "commander": "^8.3.0"
  },
  "bugs": {
    "url": "https://github.com/F2BEAR/passwordy/issues"
  },
  "homepage": "https://github.com/F2BEAR/passwordy/blob/master/README.md",
  "keywords": [
    "password",
    "cli",
    "password-generator",
    "node.js"
  ],
  "devDependencies": {
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "semantic-release": "^19.0.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
